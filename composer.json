{
  "name": "ntriga/pimcore-asset-thumbnails",
  "version": "11.2.0",
  "description": "Generates thumbnails of assets so they don't need to be generated on the fly",
  "keywords": [
    "ntriga",
    "pimcore",
    "deepl"
  ],
  "homepage": "https://github.com/ntriga/pimcore-asset-thumbnails",
  "license": "MIT",
  "authors": [
    {
      "name": "Joey Daems",
      "email": "joey@ntriga.agency",
      "role": "Developer"
    },
    {
      "name": "Vincent Bibauw",
      "email": "vincent@ntriga.agency",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "pimcore/pimcore": "^10.0 || ^11.2"
  },
  "autoload": {
    "psr-4": {
      "Ntriga\\PimcoreAssetThumbnails\\": "src"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "phpstan/extension-installer": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
